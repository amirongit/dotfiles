{
    "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim/init.lua",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.neovimClean": false,
    "window.titleBarStyle": "custom",
    "window.dialogStyle": "custom",
    "files.autoSave": "onWindowChange",
    "editor.renderWhitespace": "all",
    "editor.codeActionWidget.includeNearbyQuickFixes": false,
    "editor.codeActionWidget.showHeaders": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.fontSize": 18,
    "terminal.integrated.fontSize": 16,
    /* "editor.fontFamily": "Agave", */
    /* "editor.fontFamily": "Syne Mono", */
    /* "editor.fontFamily": "Recursive Mn Lnr St",
    "editor.fontLigatures": "'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss08', 'ss10', 'ss11', 'ss12'", */
    /* "editor.fontFamily": "Recursive Mn Csl St",
    "editor.fontLigatures": "'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss08', 'ss10', 'ss11', 'ss12'", */
    /* "editor.fontFamily": "CaskaydiaCove Nerd Font",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss20'", */
    /* "editor.fontFamily": "VictorMono Nerd Font",
    "editor.fontLigatures": "'ss01', 'ss04', 'ss06'", */
    /* "editor.fontFamily": "Iosevka Medium Extended",
    "editor.fontLigatures": "'dlig', 'ss12'", */
    "editor.fontFamily": "FiraCode Nerd Font",
    "editor.fontLigatures": "'cv01', 'cv02', 'zero', 'cv11', 'cv14', 'onum', 'cv16', 'cv29', 'cv31'",
    /* "editor.fontFamily": "JetBrainsMono Nerd Font",
    "editor.fontLigatures": "'cv02', 'cv03', 'cv04', 'cv05', 'cv06', 'cv07', 'cv08', 'cv10', 'cv11', 'cv12', 'cv14', 'cv15', 'cv16', 'cv17', 'cv18', 'cv19', 'cv20'", */
    "editor.fontWeight": "450",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.dragAndDrop": false,
    "editor.dropIntoEditor.enabled": false,
    "editor.emptySelectionClipboard": false,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": true,
    "editor.guides.highlightActiveIndentation": "always",
    "editor.hover.above": false,
    "editor.lineNumbers": "relative",
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbar.vertical": "visible",
    "editor.minimap.enabled": false,
    "editor.acceptSuggestionOnEnter": "on",
    "workbench.iconTheme": "rose-pine-icons",
    "workbench.tips.enabled": false,
    "workbench.editor.scrollToSwitchTabs": true,
    "zenMode.hideLineNumbers": false,
    "zenMode.centerLayout": false,
    "zenMode.showTabs": "none",
    "window.autoDetectColorScheme": false,
    "workbench.startupEditor": "none",
    "editor.wordWrap": "on",
    "editor.formatOnSave": true,
    "editor.codeLens": true,
    "editor.lightbulb.enabled": "off",
    "git.confirmSync": false,
    "terminal.integrated.enablePersistentSessions": false,
    "workbench.editor.empty.hint": "hidden",
    "editor.semanticHighlighting.enabled": true,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.stablePeek": false,
    "editor.stickyScroll.enabled": false,
    "editor.tabCompletion": "off",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.activityBar.location": "top",
    "git.openRepositoryInParentFolders": "never",
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordBasedSuggestions": "off",
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.fixAll": "explicit"
        },
    },
    "notebook.formatOnSave.enabled": true,
    "notebook.codeActionsOnSave": {
        "notebook.source.fixAll": "explicit",
        "notebook.source.organizeImports": "explicit"
    },
    "python.analysis.autoImportCompletions": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.useLibraryCodeForTypes": true,
    "pyright.disableOrganizeImports": true,
    "workbench.editor.enablePreview": false,
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleReferences": "goto",
    "extensions.autoUpdate": false,
    "window.menuBarVisibility": "compact",
    "editor.suggest.showIcons": true,
    "workbench.layoutControl.enabled": true,
    "window.commandCenter": false,
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.sideBar.location": "right",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "window.customTitleBarVisibility": "never",
    "workbench.editor.showTabs": "none",
    "http.proxy": "socks5://10.28.105.209:1080",
    "ruff.nativeServer": "on",
    "chat.disableAIFeatures": true,
    "workbench.colorTheme": "Ros√© Pine (no italics)",
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "block",
}
